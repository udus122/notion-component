{
  "name": "notion-component",
  "version": "1.0.0-alpha.1",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "",
    "lint:js": "eslint '**/*.@(js|ts|tsx)'",
    "lint:css": ":",
    "check": "tsc && prettier --list-different . && npm run lint:js && npm run lint:css && npm run test",
    "format": "prettier --write . && npm run lint:js -- --fix && npm run lint:css -- --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.3",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/addon-mdx-gfm": "^7.0.0",
    "@storybook/blocks": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@storybook/react-vite": "^7.0.0",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "semantic-release": "^21.0.1",
    "storybook": "^7.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/udus122/notion-component.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
